package pack1;
import java.util.*;


public class lab5 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter a num:");
		int num=sc.nextInt();
		if (num>0) {
			System.out.println("it is positive number");
		} else {
			System.out.println("it is negative number");
		}

	}

}
